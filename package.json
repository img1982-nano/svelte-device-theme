{
  "name": "@jill64/svelte-device-theme",
  "description": "A simple store that syncs with your device's theme settings",
  "version": "1.1.1",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-device-theme/issues",
  "homepage": "https://github.com/jill64/svelte-device-theme#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-device-theme.git"
  },
  "keywords": [
    "svelte",
    "device-theme",
    "dark-theme",
    "store"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx prettier --check . && npx eslint . && npx depcheck",
    "format": "npx prettier --write ."
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint-plugin-deprecation": "2.0.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "esm-env": "1.0.0",
    "svelte": "4.2.2"
  }
}
